{
  "scripts": {
    "dev": "webpack-dev-server --env=$npm_config_env --config compiler",
    "build": "webpack --env=$npm_config_env --config compiler",
    "lint": "eslint src tst compiler",
    "test": "ava"
  },
  "dependencies": {
    "autoprefixer": "^7.0.1",
    "ava": "^0.19.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "es6-promise": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-vue": "^2.0.1",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "vue": "^2.1.10",
    "vue-loader": "^12.0.0",
    "vue-router": "^2.2.0",
    "vue-template-compiler": "^2.1.10",
    "vuex": "^2.3.1",
    "webpack": "^2.3.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:promise/recommended",
      "plugin:ava/recommended"
    ],
    "plugins": [
      "import",
      "promise",
      "vue",
      "html",
      "ava"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "compiler/webpack.config.base.js"
        }
      }
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "commonjs": true
    },
    "rules": {
      "promise/param-names": "error",
      "promise/always-return": "error",
      "promise/catch-or-return": "error",
      "react/prop-types": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
